<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>瞎扯 · 如何正确的吐槽</title>
	<link href="http://tc.housne.com/" rel="alternate"></link>
	<link href="http://tc.housne.com/rss" rel="self"></link>
	<id>http://tc.housne.com/</id>
	<updated>{{ data[0]['date'] }}</updated>
	{% for news in data %}
	<entry>
		<title>{{ news['title'] }}</title>
		<link href="http://tc.housne.com/news/{{ news['id'] }}" rel="alternate" />
		<updated>{{ news['date'] }}</updated>
		<summary type="html">
			{{ news['body'] }}
		</summary>
	</entry>
	{% end %}
</feed>